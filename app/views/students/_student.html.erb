<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= student.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= student.phone %>
  </p>

  <p>
    <strong>Course:</strong>
    <%= student.course %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= student.year %>
  </p>

</div>
